"&lt;!-- &#36;Header&#36; --&gt;\n"
"\n"
"&lt;guide link=\"<i>/doc/en/guide.xml</i>\" lang=\"<i>en</i>\"&gt;\n"
"&lt;title&gt;<i>Gentoo Documentation Guide</i>&lt;/title&gt;\n"
"\n"
"&lt;author title=\"<i>Author</i>\"&gt;\n"
