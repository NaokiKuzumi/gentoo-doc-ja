#. type: Content of: <guide><chapter><section><body><ul><li>
#: en/gentoo-sparc-faq.xml:212
#, no-wrap
msgid "Solaris boots from partition 5"
msgstr ""

#. type: Content of: <guide><chapter><section><body><p>
#: en/gentoo-sparc-faq.xml:216
#, no-wrap
msgid "Separate Drive Setup:\n"
msgstr ""

#. type: Content of: <guide><chapter><section><body><ul><li>
#: en/gentoo-sparc-faq.xml:220
#, no-wrap
msgid "Second drive is SCSI ID 1"
msgstr ""

#. type: Content of: <guide><chapter><section><body><ul><li>
#: en/gentoo-sparc-faq.xml:221
#, no-wrap
msgid "Gentoo is on SCSI ID 0"
msgstr ""

#. type: Content of: <guide><chapter><section><body><ul><li>
#: en/gentoo-sparc-faq.xml:222
#, no-wrap
