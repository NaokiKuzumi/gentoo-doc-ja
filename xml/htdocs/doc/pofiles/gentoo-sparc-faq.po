"\n"
"<comment># Gentoo/SPARC options</comment>\n"
"image = /boot/vmlinux\n"
"    label = linux\n"
"    root=/dev/sda4\n"
"\n"
"<comment># Solaris options</comment>\n"
"other = /sbus/espdma/esp/sd@1,0;1\n"
"    label = solaris\n"
